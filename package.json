{
  "name": "masbs-xposter",
  "version": "0.0.1",
  "description": "A Mastodon, BlueSky cross poster",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "npm run clean && tsc --build --declaration && npm run bundle",
    "bundle": "npm run bundle:library && npm run bundle:extension",
    "bundle:library": "webpack --config webpack.config.js",
    "bundle:extension": "node bundle-extension.js",
    "clean": "npm run clean:library && npm run clean:extension",
    "clean:library": "npx rimraf dist",
    "clean:extension": "npx rimraf dist-extension",
    "postinstall": "node postinstall.js",
    "test": "jest"
  },
  "keywords": [
    "Mastodon",
    "BlueSky",
    "cross",
    "poster"
  ],
  "author": "khyos",
  "license": "MTI",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.3",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "babel-jest": "^29.7.0",
    "eslint": "^8.51.0",
    "fs-extra": "^11.1.1",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@atproto/api": "^0.6.20",
    "masto": "^6.3.1"
  }
}
